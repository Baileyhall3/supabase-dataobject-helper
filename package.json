{
  "name": "supabase-dataobject-helper",
  "displayName": "Supabase Data Object Helper",
  "description": "An extension to simplify creating data objects from supabase for use in your VSCode project.",
  "version": "1.0.0",
  "publisher": "BHDesign",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "supabase",
    "database",
    "data",
    "crud",
    "reactive"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Baileyhall3/supabase-dataobject-helper"
  },
  "bugs": {
    "url": "https://github.com/Baileyhall3/supabase-dataobject-helper/issues"
  },
  "homepage": "https://github.com/Baileyhall3/supabase-dataobject-helper#readme",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "supabase-dataobject-helper.openConfig",
        "title": "Open Supabase Data Object Helper",
        "category": "Supabase"
      },
      {
        "command": "supabase-dataobject-helper.createDataObject",
        "title": "Create Data Object",
        "category": "Supabase"
      },
      {
        "command": "supabase-dataobject-helper.testConnection",
        "title": "Test Supabase Connection",
        "category": "Supabase"
      },
      {
        "command": "supabase-dataobject-helper.clearConfig",
        "title": "Clear Supabase Configuration",
        "category": "Supabase"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "supabase-dataobject-helper",
          "title": "Supabase Data Objects",
          "icon": "$(database)"
        }
      ]
    },
    "views": {
      "supabase-dataobject-helper": [
        {
          "icon": "$(database)",
          "type": "webview",
          "id": "supabaseDataObjectHelper.configView",
          "name": "Configuration",
          "when": "true"
        }
      ]
    },
    "menus": {
      "commandPalette": [
        {
          "command": "supabase-dataobject-helper.openConfig",
          "when": "true"
        },
        {
          "command": "supabase-dataobject-helper.createDataObject",
          "when": "true"
        },
        {
          "command": "supabase-dataobject-helper.testConnection",
          "when": "true"
        },
        {
          "command": "supabase-dataobject-helper.clearConfig",
          "when": "true"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0"
  }
}
